       },
       ville:{
           required: "Saisissez la ville de votre association",
       },
       pays:{
           required: "Saisissez le pays de votre association",
       },
       description:{
           required: "Saisissez une description de votre association",
           minlength: "La description de votre association doit comporter au minimum 10 caractères"
       },
   },
   errorElement : 'div',
   errorPlacement: function(error, element) {
     var placement = $(element).data('error');
     if (placement) {
       $(placement).append(error)
     } else {
       error.insertAfter(element);
     }
   }
});
</script>
